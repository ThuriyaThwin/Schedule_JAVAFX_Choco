<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" fx:id="pane" prefHeight="344.0" prefWidth="248.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.Login">
    <Text layoutX="18.0" layoutY="86.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Aplikasi Jadwal&#10;Perkuliahan"
          textAlignment="CENTER">
        <font>
            <Font name="Verdana" size="28.0"/>
        </font>
    </Text>
    <TextField fx:id="usernameField" layoutX="28.0" layoutY="163.0" prefHeight="31.0" prefWidth="192.0"
               promptText="Username">
        <font>
            <Font size="15.0"/>
        </font>
    </TextField>
    <Button fx:id="btnLogin" defaultButton="true" layoutX="40.0" layoutY="266.0" mnemonicParsing="false"
            onAction="#loginAction" prefHeight="31.0" prefWidth="168.0" text="Login">
        <font>
            <Font size="15.0"/>
        </font>
    </Button>
    <TextField fx:id="passwordField" layoutX="28.0" layoutY="202.0" prefHeight="31.0" prefWidth="192.0"
               promptText="Password">
        <font>
            <Font size="15.0"/>
        </font>
    </TextField>
</AnchorPane>
