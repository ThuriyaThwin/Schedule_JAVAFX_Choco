<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" fx:id="kelolaDosenPane" prefHeight="516.0" prefWidth="784.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.KelolaDosen">
    <AnchorPane id="AnchorPane" fx:id="pane" layoutX="10.0" layoutY="10.0" prefHeight="422.0" prefWidth="668.0">
        <Text layoutX="44.0" layoutY="139.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Data Dosen"
              underline="true">
            <font>
                <Font size="28.0"/>
            </font>
        </Text>
        <TextField fx:id="namaField" layoutX="16.0" layoutY="161.0" prefHeight="25.0" prefWidth="192.0"
                   promptText="Nama"/>
        <Button fx:id="btnTambah" layoutX="19.0" layoutY="197.0" mnemonicParsing="false"
                onAction="#tambahDosenAction" text="Tambah"/>
        <Button fx:id="btnUpdate" layoutX="88.0" layoutY="197.0" mnemonicParsing="false"
                onAction="#updateDosenAction" text="Update"/>
        <Button fx:id="btnHapus" layoutX="156.0" layoutY="197.0" mnemonicParsing="false"
                onAction="#hapusDosenAction" text="Hapus"/>
        <TableView fx:id="tblDataDosen" layoutX="233.0" layoutY="109.0" prefHeight="386.0" prefWidth="533.0">
            <columns>
                <TableColumn fx:id="tblKolomNama" prefWidth="532.0" text="Nama"/>
            </columns>
        </TableView>
        <Button fx:id="btnDashboard" defaultButton="true" layoutX="16.0" layoutY="35.0" mnemonicParsing="false"
                onAction="#toDashboard" prefHeight="43.0" prefWidth="192.0" text="MAIN MENU" textFill="RED">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Button>
        <Text fill="#e13434" layoutX="233.0" layoutY="71.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Aplikasi Jadwal Perkuliahan" textAlignment="CENTER" wrappingWidth="533.000002861023">
            <font>
                <Font name="System Bold" size="35.0"/>
            </font>
        </Text>
        <Text fx:id="totalData" layoutX="14.0" layoutY="475.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Total Data">
            <font>
                <Font size="20.0"/>
            </font>
        </Text>
    </AnchorPane>
</AnchorPane>
